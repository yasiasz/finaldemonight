<section id="contacto">
    <div class="container">
        <div class="row">
  			<div class="col-md-12 col-sm-12 col-xs-12">
                <div class="feature_header text-center">
                    <h3 class="feature_title"> <b>Contactanos</b></h3>
                    <h4 class="feature_sub">Dinos qué necesitas!</h4>
                    <div class="divider"></div>
                </div>
  			</div>
        </div>
        <div class="row text-center">
             <div class="contact_full">
                <div class="col-md-6 col-sm-offset-3">
                    <div class="left_contact">
                      <%= form_for @message, url: contact_path do |message_form| %>
                      <fieldset>
                        <div class="form-level">
                          <%= message_form.text_field :name, placeholder: "Nombre", class: 'input-block' %>
                          <span class="form-icon fa fa-user"></span>
                        </div>
                        
                        <div class="form-level">
                          <%= message_form.email_field :email, placeholder: "Email"%>
                          <span class="form-icon fa fa-at"></span>
                        </div>
                        <div class="form-level">
                          <%= message_form.email_field :telefono, placeholder: "Telefono"%>
                          <span class="form-icon fa fa-phone"></span>
                        </div>
                        <div class="form-level">
                          <%= message_form.text_field :content, placeholder: "Message", class: 'input-block' %>
                          <span class="form-icon fa fa-envelope-o"></span>
                        </div>
                      </fieldset>
                      
                      <fieldset>
                        <div class="col-md-12 text-center">
                          <%= message_form.submit "Enviar", class: "btn btn-main featured" %>
                        </div>
                      </fieldset>
                    <% end %>
                    
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

